<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.myproject.repository.UserMapper">
    <select id="selectUser" resultType="com.project.myproject.model.User">
        select * from user
    </select>
    <insert id="insertUser" parameterType="com.project.myproject.model.User">
        insert into user values(default,#{email},#{name},#{password})
    </insert>
</mapper>